{
    "Nieuwe werkwijze aanmaken": {
        "prefix": ["werkwijze"],
        "body": ["werkwijze ${1:naam}() {", "\t$0", "}"],
        "description": "Een nieuwe werkwijze."
    },
    "Nieuwe structuur aanmaken": {
        "prefix": ["structuur"],
        "body": ["structuur ${1:naam} {", "\t$0", "}"],
        "description": "Een nieuwe structuur."
    },
    "Itereren met volg in reeks": {
        "prefix": ["volg", "volg-reeks"],
        "body": ["volg ${1:element} in reeks(${2:start}, ${3:eind}) {", "\t$0", "}"],
        "description": "Volg in reeks."
    },
    "Voorwaardelijk een sectie uitvoeren": {
        "prefix": ["als"],
        "body": ["als ${1:conditie} {", "\t${0:dan}", "}"],
        "description": "Volg in reeks."
    }
}
