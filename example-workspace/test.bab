uitbreiding<T> Doorloper<T> op Lijst<T> {
    werkwijze lengte() -> g32 {
        bekeer dit.lengte();
    }

    werkwijze krijg(plaats: g32) -> T {
        bekeer dit.inhoud[plaats];
    }
}

stel lijst = nieuw Lijst<Slinger> {};
lijst.voegToe("Abc");
lijst.voegToe("Ghi");

stel naald = "Abc";
stel b = lijst.bevat(naald);
schrijf(€"Bevat {b}");

stel s = lijst.lengte();
schrijf(€"Lengte: {s}");
